{
  "name": "server",
  "version": "0.0.0",
  "scripts": {
    "build": "tsc --build --clean && tsc",
    "lint": "eslint src/**/*.ts",
    "start": "npm run lint && npm run build && npm run dev",
    "test": "npm run build && ava",
    "dev": "node ./dist/src/index.js"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.0",
    "mysql": "^2.18.1",
    "node-pexels": "^2.0.1",
    "pexels": "^1.4.0",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@ava/typescript": "^2.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^16.7.2",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "ava": "^3.15.0",
    "eslint": "^7.32.0",
    "fs-extra": "^10.0.0",
    "typescript": "~4.3"
  },
  "ava": {
    "files": [
      "test/*"
    ],
    "timeout": "1m",
    "typescript": {
      "rewritePaths": {
        "test/": "dist/test/"
      }
    }
  }
}
